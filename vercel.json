{
    "regions": [
        "icn"
    ],
    "routes": [
        {
            "src": "/",
            "status": 301,
            "headers": {
                "Location": "https://hdml.kr/"
            }
        },
        {
            "src": "/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "X-Powered-By": "HGSEO"
            },
            "dest": "/$1"
        },
        {
            "src": "/(.*).json",
            "status": 404,
            "dest": "/errors/404.json"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/v(?<version>[2-3])/app/",
            "dest": "/data.v$version.json"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/errors/404.json"
        }
    ]
}