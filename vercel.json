{
    "regions": [
        "icn"
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "https://hdml.kr/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/v(?<version>[2-3])/app/",
            "destination": "/data.v$version.json"
        },
        {
            "source": "/(.*)",
            "destination": "/errors/404.json",
            "status": 404
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "X-Powered-By",
                    "value": "HGSEO"
                }
            ]
        }
    ]
}